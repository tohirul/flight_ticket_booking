{
  "name": "ticket-booking",
  "version": "1.0.0",
  "description": "This is a basic server for Airport Ticket Booking",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "server": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --project tsconfig.json --require tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "test": "jest",
    "db:migrate": "npx prisma format && npx prisma migrate dev",
    "prettier:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "prettier:fix": "prettier --write ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.13.1",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.12.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "prisma": "^6.4.1",
    "supertest": "^7.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@types/lodash.merge": "^4.6.9",
    "@types/morgan": "^1.9.9",
    "dotenv-expand": "^12.0.1",
    "express": "^4.21.2",
    "http-status": "^2.1.0",
    "jod": "^0.0.0",
    "lodash.merge": "^4.6.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.2.5",
    "tsyringe": "^4.10.0",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  }
}
